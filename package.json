{
  "name": "clip-diff",
  "displayName": "clip-diff",
  "publisher": "mag911",
  "author": {
    "name": "mag911"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mag911/vscode-clip-diff.git"
  },
  "description": "VSCode extension to apply diff patch directly from the clipboard",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:clip-diff.applyDiffPatchFromClipboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clip-diff.applyDiffPatchFromClipboard",
        "title": "Apply diff patch directly from the clipboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
